{
  "parser": "babel-eslint",
  "extends": ["plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "plugins": ["react", "jest", "jsx-a11y", "import"],
  "globals": {
    "graphql": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
